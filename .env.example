# Supabase Configuration
# Copy this file to .env.local and fill with your actual values
# NEVER commit .env.local to Git

# Demo Mode (true pour démo sans backend, false pour production Supabase)
NEXT_PUBLIC_DEMO_MODE=true

# Supabase (Production uniquement, laisser vide si DEMO_MODE=true)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# Service Role Key (optionnel, server-side uniquement, high privileges)
# ATTENTION: JAMAIS exposer côté client
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
